# Discord Bot Configuration (REQUIRED)
BOT_TOKEN=your_discord_bot_token_here
MONGO_URI=mongodb://host.docker.internal:27017

# AI and Translation Features (OPTIONAL)
GEMINI_KEY=your_google_gemini_api_key
TRANSLATE_KEY=your_translation_api_key

# Music Features - Spotify Integration (OPTIONAL)
SPOTIFY_CLIENT_ID=your_spotify_client_id
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret

#Optional - PG Integration
X_CLIENT_ID=your_twitter_x_client_id
X_CLIENT_SECRET=your_twitter_x_client_secret

# Bot Configuration (OPTIONAL - defaults provided)
PREFIX=&
MODE=production
SHARDS=1
VERSION=2.1.0

# Music Server Configuration (OPTIONAL - defaults provided)
LAVALINK_VERSION=4.1.1
LOCAL_LAVA=true
